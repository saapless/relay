{
  "compilerOptions": {
    "noEmit": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "target": "es2022",
    "verbatimModuleSyntax": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "moduleDetection": "force",
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "moduleResolution": "bundler",
    "module": "ES2022",
    "jsx": "preserve",
    "lib": [
      "es2022",
      "dom"
    ],
  },
}